{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/annotations": "^2.0.1",
        "doctrine/doctrine-bundle": "^2.11.1",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.0",
        "scheb/2fa-bundle": "^7.2",
        "scheb/2fa-google-authenticator": "^7.2",
        "stof/doctrine-extensions-bundle": "^1.10.1",
        "symfony/console": "7.0.*",
        "symfony/dotenv": "7.0.*",
        "symfony/flex": "^2.4.3",
        "symfony/form": "7.0.*",
        "symfony/framework-bundle": "7.0.*",
        "symfony/mime": "7.0.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/runtime": "7.0.*",
        "symfony/security-bundle": "7.0.*",
        "symfony/serializer": "7.0.*",
        "symfony/translation": "7.0.*",
        "symfony/twig-bundle": "7.0.*",
        "symfony/uid": "7.0.*",
        "symfony/ux-cropperjs": "^2.14.0",
        "symfony/ux-dropzone": "^2.14.0",
        "symfony/ux-live-component": "^2.14.1",
        "symfony/ux-turbo": "^2.14.0",
        "symfony/ux-twig-component": "^2.14.0",
        "symfony/validator": "7.0.*",
        "symfony/webpack-encore-bundle": "^2.1.1",
        "symfony/yaml": "7.0.*",
        "twig/extra-bundle": "^2.12|^3.8",
        "twig/twig": "^2.12|^3.8"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "SimpleWebApps\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleWebApps\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "check": [
            "php-cs-fixer fix --dry-run --allow-risky=yes",
            "phpcs -s",
            "psalm",
            "phan"
        ],
        "fix": [
            "php-cs-fixer fix --allow-risky=yes"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.0.*"
        }
    },
    "require-dev": {
        "ext-ast": "*",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "doctrine/doctrine-fixtures-bundle": "^3.5.1",
        "friendsofphp/php-cs-fixer": "^3.49",
        "phan/phan": "^5.4.3",
        "phpmd/phpmd": "^2.15",
        "psalm/plugin-symfony": "^5.1",
        "rarst/phpcs-cognitive-complexity": "^0.2.0",
        "squizlabs/php_codesniffer": "*",
        "symfony/css-selector": "7.0.*",
        "symfony/debug-bundle": "7.0.*",
        "symfony/maker-bundle": "^1.53",
        "symfony/phpunit-bridge": "^7.0.3",
        "symfony/stopwatch": "7.0.*",
        "symfony/web-profiler-bundle": "7.0.*",
        "vimeo/psalm": "^5.21.1",
        "weirdan/doctrine-psalm-plugin": "^2.9"
    }
}
